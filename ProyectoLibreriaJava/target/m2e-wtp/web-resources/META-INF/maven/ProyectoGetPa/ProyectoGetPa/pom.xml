<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>ProyectoGetPa</groupId>
  <artifactId>ProyectoGetPa</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>war</packaging>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <release>20</release>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>3.2.3</version>
      </plugin>
    </plugins>
  </build>
  <dependencies>
	  <dependency>
		<!--Coger los datos de la api-->
		<groupId>com.fasterxml.jackson.core</groupId>
    	<artifactId>jackson-databind</artifactId>
    	<version>2.12.5</version> <!-- Verifica la última versión en el repositorio de Maven -->
	</dependency>
	  <!-- https://mvnrepository.com/artifact/com.sun.mail/javax.mail -->
	  	<dependency>
			  <groupId>com.sun.mail</groupId>
			 <artifactId>javax.mail</artifactId>
			<version>1.6.2</version>
	  	</dependency>
	  	<dependency>
	  		<groupId>com.glezmanz</groupId>
	  		<artifactId>gestorBibliotecaJakarta</artifactId>
	  		<version>0.0.1-SNAPSHOT</version>
	  	</dependency>
  </dependencies>
</project>